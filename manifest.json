{
  "name": "Notion ID",
  "description": "Copies the Notion pages' ID into clipboard",
  "version": "0.6.2",
  "manifest_version": 3,
  "permissions": [],
  "icons": {
    "16": "/images/16-icon.png",
    "32": "/images/32-icon.png",
    "48": "/images/48-icon.png",
    "128": "/images/128-icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/16-icon.png",
      "32": "/images/32-icon.png",
      "48": "/images/48-icon.png",
      "128": "/images/128-icon.png"
    }
  }
}